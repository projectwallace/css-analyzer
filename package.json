{
  "name": "@projectwallace/css-analyzer",
  "version": "2.4.0",
  "author": "Bart Veneman",
  "repository": {
    "type": "git",
    "url": "https://github.com/projectwallace/css-analyzer"
  },
  "homepage": "https://www.projectwallace.com/oss",
  "issues": "https://github.com/projectwallace/css-analyzer/issues",
  "license": "MIT",
  "engines": {
    "node": ">=8.0"
  },
  "keywords": [
    "projectwallace",
    "wallace",
    "css",
    "stats",
    "statistics",
    "analytics",
    "cssstats",
    "parker",
    "performance",
    "styleguide",
    "metrics"
  ],
  "main": "src/analyzer/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "xo && nyc ava test"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "operator-linebreak": "off"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "useTabs": false
        }
      }
    ]
  },
  "dependencies": {
    "brotli-size": "0.0.3",
    "color-sorter": "^3.0.0",
    "css-at-supports-browser-h4cks-analyzer": "^1.0.0",
    "css-media-query-browser-h4cks-analyzer": "^1.0.0",
    "css-property-browser-h4cks-analyzer": "^1.1.0",
    "css-selector-browser-h4cks-analyzer": "^1.1.0",
    "css-shorthand-expand": "^1.2.0",
    "css-unit-sort": "^1.1.1",
    "css-value-browser-h4cks-analyzer": "^1.0.1",
    "flat": "^4.1.0",
    "gzip-size": "^5.1.0",
    "is-vendor-prefixed": "^1.0.0",
    "path": "^0.12.7",
    "postcss": "^7.0.14",
    "postcss-values-parser": "^3.0.2",
    "specificity": "^0.4.1",
    "split-lines": "^2.0.0",
    "string-natural-compare": "^2.0.3",
    "tinycolor2": "^1.4.1",
    "vendor-prefixes": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "coveralls": "^3.0.2",
    "nyc": "^13.3.0",
    "xo": "^0.24.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
