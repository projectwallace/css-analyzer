{
  "name": "@projectwallace/css-analyzer",
  "version": "1.5.2",
  "author": "Bart Veneman",
  "homepage": "https://www.projectwallace.com/oss",
  "issues": "https://github.com/projectwallace/css-analyzer/issues",
  "license": "MIT",
  "engines": {
    "node": ">=8.0"
  },
  "keywords": [
    "projectwallace",
    "wallace",
    "css",
    "stats",
    "statistics",
    "analytics",
    "cssstats",
    "parker",
    "performance",
    "styleguide",
    "metrics"
  ],
  "scripts": {
    "test": "xo && nyc ava test/analyzer test/parser"
  },
  "xo": {
    "space": true,
    "semicolon": false
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "useTabs": false
        }
      }
    ]
  },
  "dependencies": {
    "color-sorter": "^2.2.0",
    "css-color-names": "^1.0.0",
    "css-shorthand-expand": "^1.2.0",
    "css-unit-sort": "^1.1.1",
    "css-value-browser-h4cks-analyzer": "^1.0.1",
    "path": "^0.12.7",
    "postcss": "^7.0.2",
    "postcss-values-parser": "^2.0.0",
    "specificity": "^0.4.1",
    "string-natural-compare": "^2.0.3",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "coveralls": "^3.0.2",
    "nyc": "^13.1.0",
    "xo": "^0.21.1"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
