{
	"name": "@projectwallace/css-analyzer",
	"description": "The best CSS analyzer out there. Check design tokens, complexity, specificity, performance and more.",
	"version": "9.2.0",
	"author": "Bart Veneman",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/projectwallace/css-analyzer.git"
	},
	"homepage": "https://github.com/projectwallace/css-analyzer",
	"issues": "https://github.com/projectwallace/css-analyzer/issues",
	"license": "MIT",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/css-analyzer.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"types": "./dist/index.d.ts",
		"default": "./dist/css-analyzer.js"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"lint": "oxlint --config .oxlintrc.json",
		"lint-package": "publint",
		"test": "vitest",
		"check": "tsc --noEmit",
		"build": "vite build",
		"knip": "knip"
	},
	"keywords": [
		"projectwallace",
		"wallace",
		"css",
		"stylesheet",
		"stats",
		"statistics",
		"analytics",
		"performance",
		"styleguide",
		"metrics",
		"designsystem",
		"fonts",
		"colors",
		"quality",
		"code"
	],
	"dependencies": {
		"@projectwallace/css-parser": "^0.13.1"
	},
	"devDependencies": {
		"@codecov/vite-plugin": "^1.9.1",
		"c8": "^10.1.3",
		"knip": "^5.83.1",
		"oxlint": "^1.43.0",
		"prettier": "^3.6.2",
		"publint": "^0.3.17",
		"typescript": "^5.9.3",
		"vite": "^6.3.4",
		"vite-plugin-dts": "^4.5.4",
		"vitest": "^4.0.18"
	}
}
